@{
    ViewData["Title"] = "Help Desk Support Dashboard";
    Layout = "_LayoutNew";
}
<link href="~/v2.0/helpdesk/helpdesk.css" rel="stylesheet" />
<script src="https://fastly.jsdelivr.net/npm/echarts@5.4.3/dist/echarts.min.js"></script>

<div class="main-box">
    <div class="header-image">
        <div class="energy-header">
            <div class="navigation-box">
                <div class="stepper-wrapper">
                    <div class="stepper-item">
                        <div class="step-counter active">Hot <span class="material-symbols-outlined" id="check">check_circle</span></div>
                    </div>
                    <div class="stepper-item">
                        <a href="#" style="text-decoration: none;"><div class="step-counter warm">Warm</div></a>
                    </div>
                    <div class="stepper-item">
                        <a href="#" style="text-decoration: none;"><div class="step-counter cold">Cold</div></a>
                    </div>
                </div>
            </div>
            <div class="report">
                <a href="#"><button class="report-button">Trends & Reports</button></a>
            </div>
        </div>
    </div>
    <div class="content-area">
        <div class="row1">
            <div class="row1_a">
                <div class="a_header">
                    <div class="icon-h">
                        <a href="#" style="text-decoration: none;"><img src="~/v2.0/common/images/Icons/8.png" alt="" class="icon"></a>
                        <div class="img1-button">New Tickets vs Closed</div>
                    </div>
                </div>
                <div class="ntvc-container">
                    <div class="chart-containerntvc" id="ntvc"></div>
                </div>
            </div>
            <div class="row1_b">
                <div class="b_header">
                    <div class="icon-h">
                        <a href="#" style="text-decoration: none;"><img src="~/v2.0/common/images/Icons/8.png" alt="" class="icon"></a>
                        <div class="img1-button">Top Ticket Solver</div>
                    </div>
                </div>
                <div class="table-box" style="padding: .5rem;">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th scope="col">Name</th>
                                <th scope="col">Ticket Solver</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
