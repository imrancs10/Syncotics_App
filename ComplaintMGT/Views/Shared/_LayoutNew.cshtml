        }

        function alerts() {
            $.ajax({
                type: "post",
                url: "/Deshboard/GetAlerts",
                data: {},
                success: function (data) {

                    var myJSON = JSON.parse(data)

                    var txthtm = '';
                    for (var i = 0; i < myJSON.length; i++) {

                        txthtm += ' <li class="media">';
                        txthtm += '  <div class="media-body">';
                        txthtm += '  <div class="media-title">';
                        txthtm += '   <a href="#">';
                        txthtm += '  <span class="font-weight-semibold">' + myJSON[i].Name + '</span>';
                        txthtm += '  <span class="text-muted float-right font-size-sm">' + myJSON[i].Time + '</span>';
                        txthtm += '  </a>';
                        txthtm += '  </div>';
                        txthtm += '  <span class="text-muted">' + myJSON[i].Temp_in_degree + '</span>';
                        txthtm += '  </div>';
                        txthtm += '</li>';

                    }
                    $('#alerts').append(txthtm);
                    $('#alertsCount').append('<span class="badge badge-pill bg-warning-400 ml-auto ml-md-0">' + myJSON.length + '</span>');

                }
            });
        }
        function logoutapp() {
            //var MyData = { menu: [], submenu: [] };
            //localStorage.saveData = JSON.stringify(MyData);
            localStorage.clear();
