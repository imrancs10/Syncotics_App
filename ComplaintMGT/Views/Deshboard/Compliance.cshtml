
@{
    ViewData["Title"] = "Compliance";
}

<!-- Page header -->
<div class="page-header page-header-light">
    <div class="page-header-content header-elements-md-inline">
        <div class="page-title d-flex">
            <h4><i class="icon-arrow-left52 mr-2"></i> <span class="font-weight-semibold">Compliance</span> </h4>
            <a href="#" class="header-elements-toggle text-default d-md-none"><i class="icon-more"></i></a>
        </div>

        <div class="header-elements d-none">
            <div class="d-flex justify-content-center">
                <div class="breadcrumb">
                    <a href="#" class="breadcrumb-item"><i class="icon-home2 mr-2"></i> Home</a>

                    <span class="breadcrumb-item active">Compliance</span>
                </div>

                <a href="#" class="header-elements-toggle text-default d-md-none"><i class="icon-more"></i></a>
            </div>
        </div>
    </div>


</div>
<!-- /page header -->
<!-- Content area -->
<div class="content">
    <div class="order-2 order-md-1"></div>
    <div class="row">



        <div class="col-xl-12">

            <div class="card">
                <div class="card-header header-elements-inline">
                    <h5 class="card-title">
                        Overall Compliance of Store
                    </h5>

                </div>
                <div class="card-body">
                    <div class="chart-container">
                        <div class="chart has-fixed-height" id="bars_float2"></div>
                    </div>
                    <div class="chart-container">
                        <div class="chart has-fixed-height" id="bars_float"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

